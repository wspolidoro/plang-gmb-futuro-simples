            color: white;
            border: 2px solid white;
        }

        .services-section {
            background: #f8f9fa;
            border-radius: 12px;
            padding: 2rem;
            margin: 2rem 0;
        }

        .service-item {
            background: white;
            border-radius: 8px;
            padding: 1.5rem;
            margin-bottom: 1rem;
            border-left: 4px solid #2196f3;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .service-info h4 {
            margin-bottom: 0.5rem;
            color: #2c3e50;
        }

        .service-price {
